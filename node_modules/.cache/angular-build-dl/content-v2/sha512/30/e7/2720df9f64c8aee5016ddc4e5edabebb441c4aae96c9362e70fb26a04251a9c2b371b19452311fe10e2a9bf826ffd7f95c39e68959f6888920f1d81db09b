      var _c1 = ["*"];
      /** Navigation service token */

      var ReadmeNavigator = function ReadmeNavigator() {
        _classCallCheck(this, ReadmeNavigator);
      };

      var ReadmeComponent = /*#__PURE__*/function () {
        function ReadmeComponent(navigator) {
          _classCallCheck(this, ReadmeComponent);

          this.navigator = navigator;
          this.segments = [];
          /** Emits the url to navigate to */

          this.nav = new _angular_core__WEBPACK_IMPORTED_MODULE_0__["EventEmitter"]();

          if (!!navigator && typeof navigator.navigate !== 'function') {
            throw new Error('Invalid ReadmeNavigator provided!');
          }
        }
        /** Plain text souce input */


        _createClass(ReadmeComponent, [{
          key: "parse",
          set: function set(source) {
            this.compile(source);
          } // Tracking function to render the segments by content preventing re-rendering segments where content is unchanged

        }, {
          key: "trackByFn",
          value: function trackByFn(index, item) {
            return item.type + item.content || '' + item.url || '';
          }
          /** Compiles the input text into segments */

        }, {
          key: "compile",
          value: function compile(source) {
            // Resets the segments array
            this.segments.splice(0); // Skips null or emptiìy sources

            if (!source) {
              return this.segments;
            }
            /** Parsing regex:
             * 1st - Styling match - "([*+_~])(.*?[^\\])\1"
             * Matches sequences starting with '*', '+', '_' or '~' end ending with the very same character as soon as it isn't preceeded by a '\'
             * to render <b>, <i>, <u> or <s> elements respectively.
             * 2nd - Anchor match - "\[(.*?)\]\((.+?)\)"
             * matches text within square brakets followed by an url within round brakets '[text](url)' to render as anchors <a> elements.
             * 3rd - Escape match - Matches backslashes followed by one of the special chars to renders the char as it is.
             * 4th - Break match: '\n'
             * matches the newline charachers to render <br> elements
             */


            var start = 0;
            var match;
            var rx = /([*+_~])(.*?[^\\])\1|\[(.*?)\]\((.+?)\)|\\([*+_~])|\n/g;

            while (match = rx.exec(source)) {
              // Prevents the zero-length match infinite loop for all browsers
              if (match.index == rx.lastIndex) {
                rx.lastIndex++;
              }

              ; // Pushes the text preceeding a match 

              if (match.index > start) {
                var content = source.substring(start, match.index);
                this.segments.push({
                  type: "text",
                  content: content
                });
              } // Breaks


              if (match[0] === '\n') {
                this.segments.push({
                  type: "break"
                });
              } // Styled texts
              else if (match[1]) {
                  var type = match[1] === '*' ? 'bold' : match[1] === '+' ? 'italic' : match[1] === '_' ? 'underline' : 'strikethrough';
                  this.segments.push({
                    type: type,
                    content: match[2]
                  });
                } // Links
                else if (match[3]) {
                    this.segments.push({
                      type: "link",
                      content: match[3],
                      url: match[4]
                    });
                  } // Escapes
                  else if (match[5]) {
                      this.segments.push({
                        type: "text",
                        content: match[5]
                      });
                    }

              ; // Moves to the next text segment

              start = match.index + match[0].length;
            } // Appends the final text chunk 


            if (rx.lastIndex < source.length) {
              var _content = source.substring(start, source.length);

              this.segments.push({
                type: "text",
                content: _content
              });
            }

            return this.segments;
          }
          /** True whenever the navigate output is used */

        }, {
          key: "eventNavigation",
          get: function get() {
            return this.nav.observers.length > 0;
          }
          /** True whenever a ReadmeNavigator has been provided */

        }, {
          key: "serviceNavigation",
          get: function get() {
            return !!this.navigator;
          }
          /** Navigates to the specified url */

        }, {
          key: "navigate",
          value: function navigate(url) {
            // Emits on the navigate output
            if (this.eventNavigation) {
              return this.nav.emit(url), false;
            } // Calls upon the ReadmeNavigator


            if (this.serviceNavigation) {
              return this.navigator.navigate(url), false;
            } // No navigation handlers...


            console.log("Navigation missed:", url); // Prevents default

            return false;
          }
        }]);

        return ReadmeComponent;
      }();

      ReadmeComponent.ɵfac = function ReadmeComponent_Factory(t) {
        return new (t || ReadmeComponent)(_angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵdirectiveInject"](ReadmeNavigator, 8));
      };

