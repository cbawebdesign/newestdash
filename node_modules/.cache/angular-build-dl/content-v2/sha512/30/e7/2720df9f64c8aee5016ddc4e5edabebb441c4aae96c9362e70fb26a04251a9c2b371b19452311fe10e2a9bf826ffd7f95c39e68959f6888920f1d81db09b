    /*! exports provided: resolveRoutesWithContent, flatten, ContentRouterModule */

    /***/
    function J3Me(module, __webpack_exports__, __webpack_require__) {
      "use strict";

      __webpack_require__.r(__webpack_exports__);
      /* harmony export (binding) */


      __webpack_require__.d(__webpack_exports__, "resolveRoutesWithContent", function () {
        return resolveRoutesWithContent;
      });
      /* harmony export (binding) */


      __webpack_require__.d(__webpack_exports__, "flatten", function () {
        return flatten;
      });
      /* harmony export (binding) */


      __webpack_require__.d(__webpack_exports__, "ContentRouterModule", function () {
        return ContentRouterModule;
      });
      /* harmony import */


      var _angular_core__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(
      /*! @angular/core */
      "8Y7J");
      /* harmony import */


      var _angular_router__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(
      /*! @angular/router */
      "iInd");
      /* harmony import */


      var _loader_content_configurator_service__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(
      /*! ../loader/content-configurator.service */
      "GQmz");
      /* harmony import */


      var _content_resolver__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(
      /*! ./content-resolver */
      "aLL1");
      /* harmony import */


      var _content_module__WEBPACK_IMPORTED_MODULE_4__ = __webpack_require__(
      /*! ../content.module */
      "w9+7"); // Walks doen the routes' tree applying content resolvers for the requested contents


      function resolveRoutesWithContent(routes, source) {
        var selector = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : 'lang';

        // Skips empty routes
        if (!routes) {
          return [];
        } // Maps the incoming routes


        return routes.map(function (route) {
          // Grabs the source path from the route, when defined. Defaults to the global path otherwise.
          var path = route.source || source; // Whenever content is defined...

          if (!!route.content) {
            // Gets the content array
            var content = typeof route.content === 'string' ? [route.content] : route.content; // Gets the resolve data map

            var resolve = route.resolve || {}; // Builds the content resolvers map

            route.resolve = content.reduce(function (resolve, file) {
              // Adds a resolver for the given name 
              resolve[file] = _content_resolver__WEBPACK_IMPORTED_MODULE_3__["ContentResolver"].create(path, file, ContentRouterModule); // Next

              return resolve;
            }, resolve); // Removes the source property from the route configuration

            if (!!route.source) {
              delete route.source;
            } // Removes the content property from the route


            delete route.content;
          } // Walk down to children


          if (!!route.children) {
            route.children = resolveRoutesWithContent(route.children, source, selector);
          } // Done


          return route;
        });
      } // Helper to flateten the multi ROUTES


      function flatten(arr) {
        return Array.prototype.concat.apply([], arr);
      }
      /** Router module with dynamic content loading  */


      var ContentRouterModule = /*#__PURE__*/function () {
        function ContentRouterModule(routes, config) {
          _classCallCheck(this, ContentRouterModule);

          // Parses the routes adding content resolvers on loading beforing the actual routing accurs
          // This approach works both with JIT and AOT
          var resolvedRoutes = resolveRoutesWithContent(flatten(routes), config.source, config.selector);
        }
        /** Initializes child routes with content */


        _createClass(ContentRouterModule, null, [{
          key: "forChild",
          value: function forChild(routes) {
            return {
              ngModule: ContentRouterModule,
              providers: [Object(_angular_router__WEBPACK_IMPORTED_MODULE_1__["provideRoutes"])(routes)]
            };
          }
        }]);

        return ContentRouterModule;
      }();

      ContentRouterModule.ɵmod = _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵdefineNgModule"]({
        type: ContentRouterModule
      });
      ContentRouterModule.ɵinj = _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵdefineInjector"]({
