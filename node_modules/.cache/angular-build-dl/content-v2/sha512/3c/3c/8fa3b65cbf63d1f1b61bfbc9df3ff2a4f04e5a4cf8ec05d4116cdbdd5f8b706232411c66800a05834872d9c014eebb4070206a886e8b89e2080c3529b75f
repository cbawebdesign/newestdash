        Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_1__["map"])(([force]) => this.isDarkMode(force)), 
        // Filters for value changes only
        Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_1__["distinctUntilChanged"])()).subscribe(subscriber));
        this.document = document;
