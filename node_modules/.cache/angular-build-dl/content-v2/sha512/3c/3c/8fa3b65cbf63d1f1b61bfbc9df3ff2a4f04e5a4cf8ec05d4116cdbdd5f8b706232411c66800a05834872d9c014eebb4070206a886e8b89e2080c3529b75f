/** StripeCaller class implements an observable-based proxy to call the Stripe.js SDK */
class StripeCaller {
    constructor(stripejs, publicKey, options) {
        /** The optional stripe account to connect to @see [Connect](https://stripe.com/docs/connect) */
        this.account$ = new rxjs__WEBPACK_IMPORTED_MODULE_0__["BehaviorSubject"](undefined);
        // Resolves the stripe instance from the StripeContructor promise
        this.stripe$ = Object(rxjs__WEBPACK_IMPORTED_MODULE_0__["from"])(stripejs).pipe(Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_1__["switchMap"])(stripeContructor => {
            // Checks upon the stripe account to optionally connect to
            return this.account$.pipe(Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_1__["map"])(stripeAccount => stripeContructor(publicKey, Object.assign(Object.assign({}, options), { 
                // Connects to the given account, if any
                stripeAccount }))));
        }), Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_1__["shareReplay"])(1));
    }
    /**
     * For usage with [Connect](https://stripe.com/docs/connect) only.
     * Specifying a connected account ID (e.g., `acct_24BFMpJ1svR5A89k`) allows you to perform actions on behalf of that account.
     */
    connect(stripeAccount) {
        this.account$.next(stripeAccount);
    }
    /** Invokes the requested Stripe funciton */
    callStripe(signature, args) {
        // Resolves the active Stripe instance
        return this.stripe$.pipe(Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_1__["switchMap"])(stripe => {
