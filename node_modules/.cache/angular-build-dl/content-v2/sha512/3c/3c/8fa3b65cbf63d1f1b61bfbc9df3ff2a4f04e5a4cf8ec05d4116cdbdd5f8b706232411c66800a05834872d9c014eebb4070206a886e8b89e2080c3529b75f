__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "BackgroundObservable", function() { return BackgroundObservable; });
/* harmony import */ var rxjs__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! rxjs */ "qCKp");
/* harmony import */ var rxjs_operators__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! rxjs/operators */ "kU1M");
/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! @angular/core */ "8Y7J");



class BackgroundObservable extends rxjs__WEBPACK_IMPORTED_MODULE_0__["Observable"] {
    constructor() {
        super(observer => this.inner$.subscribe(observer));
        // Background style 
        this.styler$ = new rxjs__WEBPACK_IMPORTED_MODULE_0__["BehaviorSubject"](undefined);
        // Builds the innes observable to support nesting images
        this.inner$ = this.styler$.pipe(
        // Buffers the image requests
        Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_1__["scan"])((buffer, value) => {
            // Resets the buffer
            if (!value) {
                buffer = [];
            }
            // Clears a previously applied image
            else if (typeof value === 'string') {
                // Seeks for the image within the buffer and removes it
                const index = buffer.findIndex(item => item["background-image"] === value);
                if (index >= 0) {
                    buffer.splice(index, 1);
                }
            }
            // Pushes the new image into the buffer otherwise
            else {
                buffer.push(value);
            }
            return buffer;
        }, []), 
        // Always emit the last buffer item
        Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_1__["map"])(buffer => buffer[buffer.length - 1]), 
        // Ensures the style being applied according to the animationFrameScheduler (so to say in-sync with the rendering)
        // preventing the notorious ExpressionChangedAfterItHasBeenChecked exception without introducing any delay the 
        // user may otherwise perceive when navigating    
        Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_1__["observeOn"])(rxjs__WEBPACK_IMPORTED_MODULE_0__["animationFrameScheduler"]));
    }
    /** Applies the given style to the navigator's content background */
    applyBackground(style) {
        if (!style["background-image"]) {
            return undefined;
        }
        return this.styler$.next(style), style["background-image"];
    }
    /** Clears the background  */
    clearBackground(image) {
        this.styler$.next(image);
    }
}
BackgroundObservable.ɵfac = function BackgroundObservable_Factory(t) { return new (t || BackgroundObservable)(); };
