        this.ref.afterClosed().subscribe(value => {
            // Emits the dialog closed with value
            this.closedChange.emit(value);
            // Makes sure the reference goes backundefined when closed 
            this.ref = undefined;
        });
        // Returns the reference for further use
        return this.ref;
    }
