/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "flatten", function() { return flatten; });
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "ContentRouterModule", function() { return ContentRouterModule; });
/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! @angular/core */ "8Y7J");
/* harmony import */ var _angular_router__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! @angular/router */ "iInd");
/* harmony import */ var _loader_content_configurator_service__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! ../loader/content-configurator.service */ "GQmz");
/* harmony import */ var _content_resolver__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(/*! ./content-resolver */ "aLL1");
/* harmony import */ var _content_module__WEBPACK_IMPORTED_MODULE_4__ = __webpack_require__(/*! ../content.module */ "w9+7");







// Walks doen the routes' tree applying content resolvers for the requested contents
function resolveRoutesWithContent(routes, source, selector = 'lang') {
    // Skips empty routes
    if (!routes) {
        return [];
    }
    // Maps the incoming routes
    return routes.map(route => {
        // Grabs the source path from the route, when defined. Defaults to the global path otherwise.
        const path = route.source || source;
        // Whenever content is defined...
        if (!!route.content) {
            // Gets the content array
            const content = typeof (route.content) === 'string' ? [route.content] : route.content;
            // Gets the resolve data map
            const resolve = route.resolve || {};
            // Builds the content resolvers map
            route.resolve = content.reduce((resolve, file) => {
                // Adds a resolver for the given name 
                resolve[file] = _content_resolver__WEBPACK_IMPORTED_MODULE_3__["ContentResolver"].create(path, file, ContentRouterModule);
                // Next
                return resolve;
            }, resolve);
            // Removes the source property from the route configuration
            if (!!route.source) {
                delete route.source;
            }
            // Removes the content property from the route
            delete route.content;
        }
        // Walk down to children
        if (!!route.children) {
            route.children = resolveRoutesWithContent(route.children, source, selector);
        }
        // Done
        return route;
    });
}
// Helper to flateten the multi ROUTES
function flatten(arr) {
    return Array.prototype.concat.apply([], arr);
}
/** Router module with dynamic content loading  */
class ContentRouterModule {
    constructor(routes, config) {
        // Parses the routes adding content resolvers on loading beforing the actual routing accurs
        // This approach works both with JIT and AOT
        const resolvedRoutes = resolveRoutesWithContent(flatten(routes), config.source, config.selector);
    }
    /** Initializes child routes with content */
    static forChild(routes) {
        return {
            ngModule: ContentRouterModule,
            providers: [Object(_angular_router__WEBPACK_IMPORTED_MODULE_1__["provideRoutes"])(routes)]
        };
    }
}
ContentRouterModule.ɵmod = _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵdefineNgModule"]({ type: ContentRouterModule });
ContentRouterModule.ɵinj = _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵdefineInjector"]({ factory: function ContentRouterModule_Factory(t) { return new (t || ContentRouterModule)(_angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵinject"](_angular_router__WEBPACK_IMPORTED_MODULE_1__["ROUTES"]), _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵinject"](_loader_content_configurator_service__WEBPACK_IMPORTED_MODULE_2__["ContentConfigurator"])); }, imports: [[_content_module__WEBPACK_IMPORTED_MODULE_4__["ContentModule"], _angular_router__WEBPACK_IMPORTED_MODULE_1__["RouterModule"]], _content_module__WEBPACK_IMPORTED_MODULE_4__["ContentModule"], _angular_router__WEBPACK_IMPORTED_MODULE_1__["RouterModule"]] });
