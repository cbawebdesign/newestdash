    function i7Z(module, __webpack_exports__, __webpack_require__) {
      "use strict";

      __webpack_require__.r(__webpack_exports__);
      /* harmony export (binding) */


      __webpack_require__.d(__webpack_exports__, "getStripeJS", function () {
        return getStripeJS;
      });
      /* harmony export (binding) */


      __webpack_require__.d(__webpack_exports__, "loadStripeJS", function () {
        return loadStripeJS;
      });
      /** Retrives the global StripeJS object  */


      function getStripeJS() {
        return !!window ? window.Stripe : undefined;
      }
      /** Stripe.js v3 script loader. We do not use the official loader provided
       * by @stripe/stripe-js since we may need creating multiple Stripe instances
       * while connecting to multiple accounts */


      function loadStripeJS() {
        /** The official stripe-js link. The script must be loaded from the stripe server for PCI compliance */
        var STRIPE_V3_URL = 'https://js.stripe.com/v3'; // Try to get the StripeJS instance first

        var StripeJS = getStripeJS(); // Proceed in loading the script asyncronously

        return StripeJS ? Promise.resolve(StripeJS) : new Promise(function (resolve, reject) {
          try {
            // We'll be trying to get the existing script or injecting a new one
            var findScript = function findScript() {
              return document.querySelector("script[src^=\"".concat(STRIPE_V3_URL, "\"]"));
            };

            var injectScript = function injectScript() {
              var script = document.createElement('script');
              script.src = STRIPE_V3_URL;
              script.type = 'text/javascript';
              script.async = true;
              (document.head || document.body).appendChild(script);
              return script;
            }; // Seeks for the script elementn or inject a new one


            var script = findScript() || injectScript(); // Listens for error or completion

            script.addEventListener('error', function () {
              return reject(new Error("Unable to load StripeJS"));
            });
            script.addEventListener('load', function () {
              return resolve(getStripeJS());
            });
          } catch (e) {
            reject(e);
          }
        });
      }
