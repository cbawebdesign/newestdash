            throw new Error("Coounter shards must be a positive integer greater than 0");
          } // Creates a local copy of the counter 


          _this32._counter$ = new rxjs__WEBPACK_IMPORTED_MODULE_2__["BehaviorSubject"](0); // Builds the counter observable merging the local counter with the remote one to improve reactivity

          _this32.counter$ = Object(rxjs__WEBPACK_IMPORTED_MODULE_2__["merge"])( // Merges the local counter copy...
          _this32._counter$, // With the distributed counter value, keeping the local copy up to do date 
