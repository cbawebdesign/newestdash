/** Document object in the database, created by the DatabaseService */
class DatabaseDocument {
    constructor(db, pathOrRef) {
        this.db = db;
        this.ref = db.doc(pathOrRef);
    }
    /** Unwraps a document snapshot assuming its reference and returning the data content */
    unwrap(snapshot) {
        return this.ref = snapshot === null || snapshot === void 0 ? void 0 : snapshot.ref, Object(_utils__WEBPACK_IMPORTED_MODULE_0__["mapSnaphotData"])(snapshot);
    }
