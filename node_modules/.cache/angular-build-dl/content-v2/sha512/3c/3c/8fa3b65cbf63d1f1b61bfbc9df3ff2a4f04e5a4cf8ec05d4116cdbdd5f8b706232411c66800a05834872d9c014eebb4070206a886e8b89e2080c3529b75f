/*!***********************************************************!*\
  !*** ./connect/src/lib/database/collection/collection.ts ***!
  \***********************************************************/
/*! exports provided: DatabaseCollection */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "DatabaseCollection", function() { return DatabaseCollection; });
/* harmony import */ var rxjs_operators__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! rxjs/operators */ "kU1M");
/* harmony import */ var rxjs__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! rxjs */ "qCKp");
/* harmony import */ var _operators__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! ./operators */ "wSf6");
/* harmony import */ var _query__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(/*! ./query */ "McMp");




/** Collection object in the database, created by the DatabaseService */
class DatabaseCollection extends _query__WEBPACK_IMPORTED_MODULE_3__["DatabaseQuery"] {
    get ref() { return this._ref; }
    constructor(db, pathOrRef) {
        // Builds the query observalbe first
        super(db, db.col(pathOrRef));
    }
    /** Returns the collection object id */
    get id() { return this.ref.id; }
    /** Returns the collection object path relative to the database root */
    get path() { return this.ref.path; }
    /** Generates a unique ID as if to be used with a a new document */
    uniqueId() { return this.ref.doc().id; }
    /**
     * Returns the requested child document
     * @param path the document path within the collection. If no path is specified,
     * an automatically-generated unique ID will be used for the refurned DatabaseDocument
     */
    document(path) {
        return this.db.document(this.ref.doc(path));
    }
    /**
     * Adds a new document to the collection
     * @returns a promise of a DatabaseDocument
     */
    add(data) {
        const created = this.db.timestamp;
        return this.ref.add(Object.assign(Object.assign({}, data), { created })).then(ref => this.db.document(ref));
    }
    /**
     * Wipes the all the documents matching the query
     * @param query a number to limit the batch size or alternatively a query function
     * selecting the documents to wipe. Just make sure to include a limit() to avoid
     * exceeding the maximum writes limit during batch deletion.
     */
    wipe(query = 50) {
        // Builds the query function according to the input value
        const qf = (typeof query === 'number') ? qf => qf.limit(query) : query;
        // Starts by pushing whatever value
        return this.deleteBatch(qf).pipe(
        // Recursively delete the next batches 
        Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_0__["expand"])(count => count > 0 ? this.deleteBatch(qf) : rxjs__WEBPACK_IMPORTED_MODULE_1__["EMPTY"]), 
        // Accumulates the total deletion count
        Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_0__["scan"])((total, count) => total + count, 0)
        // Returns as a promise
        ).toPromise();
    }
    // Detetes documents in a batch
    deleteBatch(qf) {
        // Makes sure to limit the request up to bachSize documents
        return this.pipe(Object(_operators__WEBPACK_IMPORTED_MODULE_2__["query"])(qf), Object(_operators__WEBPACK_IMPORTED_MODULE_2__["get"])(), Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_0__["switchMap"])(snap => {
            // Skips when done
            if (snap.empty) {
                return Object(rxjs__WEBPACK_IMPORTED_MODULE_1__["of"])(0);
            }
            // Delete documents in a batch
            const batch = this.db.batch();
            snap.forEach(doc => batch.delete(doc.ref));
            // Commits the batch write and returns the snapshot size
            return batch.commit().then(() => snap.size);
        }));
    }
}


