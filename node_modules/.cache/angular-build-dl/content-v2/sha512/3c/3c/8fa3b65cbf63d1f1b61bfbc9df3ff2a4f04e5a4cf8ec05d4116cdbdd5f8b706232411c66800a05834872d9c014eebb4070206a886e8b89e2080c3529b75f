/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "StorageFolder", function() { return StorageFolder; });
/* harmony import */ var rxjs_operators__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! rxjs/operators */ "kU1M");
/* harmony import */ var _storage_reference__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! ../storage-reference */ "+hZt");
/* harmony import */ var rxjs__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! rxjs */ "qCKp");



/** Abstacts a StorageReference as a folder to navigate or upload files into */
class StorageFolder extends _storage_reference__WEBPACK_IMPORTED_MODULE_1__["StorageReference"] {
    constructor(st, path) {
        // Constructs the parent StorageReference
        super(st, path);
        /** Internal file upload subject */
        this.upload$ = new rxjs__WEBPACK_IMPORTED_MODULE_2__["BehaviorSubject"](null);
        // Tracks the source to support dynamic ref changes
        this.source$ = new rxjs__WEBPACK_IMPORTED_MODULE_2__["BehaviorSubject"](this.ref);
        // Builds an intermediate observable getting the listResult
        const result$ = this.source$.pipe(
        // Updates the folder ref
        Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_0__["tap"])(ref => this.from(ref)), 
        // Lists the folder content
        Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_0__["switchMap"])(ref => ref ? this.listAll() : null), 
        // Clean-up the upload stream when a new list comes
        Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_0__["tap"])(() => this.upload$.next(null)), 
        // Shares the result between the following folders & files
        Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_0__["shareReplay"])(1));
        // Builds an observable to list folders as StorageRef(s)
        this.folders$ = result$.pipe(Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_0__["map"])(result => result.prefixes));
        // Builds an observable to list files as StorageFile(s)
        this.files$ = result$.pipe(
        // Maps the existing items into files
        Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_0__["map"])(result => result.items.map(ref => st.file(ref))), 
        // Appends the new uploads when requested
        Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_0__["switchMap"])(files => this.upload$.pipe(
        // Starts the uploads and map it into a file
        Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_0__["map"])(upload => upload && st.file(upload)), 
        // Appends the aploads to the previous list
        Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_0__["scan"])((uploads, upload) => upload ? uploads.concat(upload) : uploads, files))));
    }
    /** List the files/folders of the given path */
    ls(path) {
        // Updates the current list from the very same path
        if (path === '.') {
            return this.source$.next(this.ref);
        }
        // Handles the '/' listing the root content
        if (path === '/') {
            return this.source$.next(this.ref.root);
        }
        // Handles the '..' listing the parent content
        if (path === "..") {
            return this.source$.next(this.ref.parent);
        }
        // Handles plain strings listing the 'subfolder' child
        if (typeof path === 'string') {
            return this.source$.next(this.ref.child(path));
        }
        // Lists the given reference otherwise
        this.source$.next(path);
    }
    /** Uploads new files in the current folder */
    upload(file, meta) {
        // Creates the UploadObservable
        const up = this.child(file.name).put(file, meta);
        // Pushes the objesvable along the uploading stream and returns it to the caller
        return this.upload$.next(up), up;
    }
}


