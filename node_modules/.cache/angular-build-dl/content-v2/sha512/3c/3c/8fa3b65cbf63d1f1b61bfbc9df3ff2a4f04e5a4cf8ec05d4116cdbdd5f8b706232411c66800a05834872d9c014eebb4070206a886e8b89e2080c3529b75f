        this.url = (typeof this.functions._url === 'function') ? this.functions._url('') :
            // Falls back computing the url as per the firebase sdk 
            ((origin ? (origin + '/') : `https://${region || 'us-central1'}-`) + `${app.options.projectId}.cloudfunctions.net/`);
    }
    /** Returns the requested callable cloud function */
    callable(name, options) {
        const callable = this.functions.httpsCallable(name, options);
        return (data) => callable(data).then(result => result.data);
    }
