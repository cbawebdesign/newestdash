        }, {
          key: "registerNew",
          value: function registerNew(email, password) {
            var displayName = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : "";
            console.log("Registering a new user: " + email); // Create a new user with email and password

            return this.auth.createUserWithEmailAndPassword(email, password) // Update the user info with the given name
            .then(function (credential) {
              return credential.user.updateProfile({
                displayName: displayName
              }) // Returns the updated User object
              .then(function () {
                return credential.user;
              });
            });
          }
          /**
           * Signs in with the given user email and password
           * @param email the email to register with
           * @param password the secret password
           * @returns the authenticated User object
           */

        }, {
          key: "signIn",
          value: function signIn(email, password) {
            console.log("Signing in as: ", email);
            return this.auth.signInWithEmailAndPassword(email, password).then(function (credential) {
              return credential.user;
            });
          }
          /** Initializes the requested provider for further use */

        }, {
          key: "setupProvider",
          value: function setupProvider(providerId) {
            switch (providerId) {
              case 'google':
              case 'google.com':
                return new firebase_app__WEBPACK_IMPORTED_MODULE_2__["auth"].GoogleAuthProvider();

              case 'facebook':
              case 'facebook.com':
                return new firebase_app__WEBPACK_IMPORTED_MODULE_2__["auth"].FacebookAuthProvider();

              case 'twitter':
              case 'twitter.com':
                return new firebase_app__WEBPACK_IMPORTED_MODULE_2__["auth"].TwitterAuthProvider();

              case 'github':
              case 'github.com':
                return new firebase_app__WEBPACK_IMPORTED_MODULE_2__["auth"].GithubAuthProvider();
            }

            return null;
          }
          /**
           * Signs in using the given provider
           * @param provider the name of the provider to sign in with
           * @returns the authenticated User object
           */

        }, {
          key: "signInWith",
          value: function signInWith(providerId) {
            console.log("Signing-in using: ", providerId); // Setup the provider 

