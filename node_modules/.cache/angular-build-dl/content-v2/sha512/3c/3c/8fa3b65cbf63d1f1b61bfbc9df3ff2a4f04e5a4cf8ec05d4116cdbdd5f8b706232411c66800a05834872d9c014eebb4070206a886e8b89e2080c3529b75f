/***/ "sAoc":
/*!****************************************************!*\
  !*** ./emoji/src/lib/utils/emoji-utils.service.ts ***!
  \****************************************************/
/*! exports provided: EmojiConfigToken, EmojiUtils */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "EmojiConfigToken", function() { return EmojiConfigToken; });
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "EmojiUtils", function() { return EmojiUtils; });
/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! @angular/core */ "8Y7J");
/* harmony import */ var _emoji_utils__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! ./emoji-utils */ "6ks8");



;
const EmojiConfigToken = new _angular_core__WEBPACK_IMPORTED_MODULE_0__["InjectionToken"]('wizdm-emoji-config');
class EmojiUtils {
    constructor(native, regex, config) {
        this.native = native;
        this.regex = regex;
        this.config = config;
        // Grabs the source path and the image extension from the configuration object
        this.filePath = this.assessPath(config && config.emojiPath) || 'assets/emoji/';
        this.fileExt = this.assessExt(config && config.emojiExt) || '.png';
    }
    assessPath(path) {
        return path ? (path.endsWith('/') ? path : (path + '/')) : '';
    }
    assessExt(ext) {
        return ext ? (ext.startsWith('.') ? ext : ('.' + ext)) : '';
    }
    /** Returns the most suitable emoji mode based on the request */
    emojiMode(mode) {
        var _a;
        switch (mode || ((_a = this.config) === null || _a === void 0 ? void 0 : _a.emojiMode) || 'auto') {
            case 'native':
                return 'native';
            case 'web':
                return 'web';
        }
        return this.native ? 'native' : 'web';
    }
    /** Computes the full path to load the image corersponding to the given emoji */
    imageFilePath(emoji) {
        if (!emoji) {
            return '';
        }
        const pts = [];
        for (let cp of emoji) {
            pts.push(cp.codePointAt(0).toString(16));
        }
        return this.filePath + pts.join('-') + this.fileExt;
    }
    isEmoji(source) {
        return this.regex.test(source);
    }
    matchEmojiCodes(source) {
        this.regex.lastIndex = 0;
        return this.regex.exec(source);
    }
    /** Parses the source text searching for emoji unicode sequences */
    parseEmojiCodes(source, callbackfn) {
        if (typeof callbackfn !== 'function') {
            throw new Error('Callback must be a funciton');
        }
        // Resets the starting position
        this.regex.lastIndex = 0;
        // Loop on RegExp matches
        let match;
        while (match = this.regex.exec(source)) {
            callbackfn(match[0], match.index);
        }
    }
}
