        _createClass(DistributedCounter, [{
          key: "accumulate",
          value: function accumulate() {
            return this.stream().pipe(Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_1__["map"])(function (counters) {
              return !!counters ? counters.reduce(function (sum, shard) {
                return sum + (shard.count || 0);
              }, 0) : 0;
            }));
          }
          /** Updates the counter by the given increment (or decrement) */

        }, {
          key: "update",
          value: function update(increment) {
            var _this33 = this;

            // Updates the local copy first to improve reactivity
            this._counter$.next(this._counter$.value + increment); // Pick a shard randomly


            var rnd = Math.floor(Math.random() * this.shards);
            var shard = this.ref.doc(rnd.toString()); // Runs the shard update within a transaction

            return this.db.transaction(function (trx) {
              // Tests if the single shard exists (1 read)
              return trx.snap(shard).then(function (snap) {
                // Updates the shard value (1 write)
                if (snap.exists) {
                  // Uses the special FieldValue as increment
                  var count = _this33.db.increment(increment); // Updates the shard (1 write)


                  return trx.update(shard, {
                    count: count
                  });
                } // Creates the shard with the given initial value (1 write)


                return trx.set(shard, {
                  count: increment
                });
              });
            });
          } // Creates the shards in a batch initializing the counter value

        }, {
          key: "create",
          value: function create() {
            var start = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : 0;
            // Uses firestore references directly
            var batch = this.db.batch(); // Inits the first shard with the starting value

            batch.set(this.ref.doc('0'), {
              count: start
            }); // Loops to create the additional shards

            for (var i = 1; i < this.shards; i++) {
              batch.set(this.ref.doc(i.toString()), {
                count: 0
              });
            } // Commit the changes


            return batch.commit();
          }
