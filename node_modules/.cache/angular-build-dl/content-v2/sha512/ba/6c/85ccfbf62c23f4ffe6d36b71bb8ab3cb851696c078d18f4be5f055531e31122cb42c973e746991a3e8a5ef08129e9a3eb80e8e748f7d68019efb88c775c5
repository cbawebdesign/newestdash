/*! exports provided: LoadWidgetDirective */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "LoadWidgetDirective", function() { return LoadWidgetDirective; });
/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! @angular/core */ "8Y7J");


class LoadWidgetDirective {
    constructor(widgets, view, resolver, injector) {
        this.widgets = widgets;
        this.view = view;
        this.resolver = resolver;
        this.injector = injector;
    }
    set widget(widget) {
        if (!widget || this.ref) {
            return;
        }
        this.loadWidget(widget);
    }
    loadWidget(widget) {
        const loader = this.widgets.find(w => w.type === widget.type);
        if (!loader) {
            return;
        }
        loader.loadComponent().then(cmp => {
            const factory = this.resolver.resolveComponentFactory(cmp);
            this.ref = this.view.createComponent(factory, null, this.injector);
            this.ref.instance.config = widget;
        });
    }
}
LoadWidgetDirective.ɵfac = function LoadWidgetDirective_Factory(t) { return new (t || LoadWidgetDirective)(_angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵdirectiveInject"]('widgets'), _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵdirectiveInject"](_angular_core__WEBPACK_IMPORTED_MODULE_0__["ViewContainerRef"]), _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵdirectiveInject"](_angular_core__WEBPACK_IMPORTED_MODULE_0__["ComponentFactoryResolver"]), _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵdirectiveInject"](_angular_core__WEBPACK_IMPORTED_MODULE_0__["Injector"])); };
