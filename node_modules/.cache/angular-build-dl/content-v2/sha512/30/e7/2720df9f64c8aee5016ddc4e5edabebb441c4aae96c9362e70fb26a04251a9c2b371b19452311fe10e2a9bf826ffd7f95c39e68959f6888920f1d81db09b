            if (authProvider === null) {
              return Promise.reject({
                code: 'auth/unsupported-provider',
                message: 'Unsupported provider'
              });
            } // Authenticate the user with the provider


            return this.auth.signInWithPopup(authProvider).then(function (credential) {
              return credential.user;
            });
          }
          /**
           * Refreshes the current authentication repeating the secret password
           * @param password the secret password
           * @returns the authenticated User object
           */

        }, {
          key: "reauthenticate",
          value: function reauthenticate(password) {
            var _this26 = this;

            // Dismiss the re-authentication attempt whenever the user isn't authenitcated
            if (!this.authenticated) {
              return Promise.reject({
                code: 'auth/user-not-authenticated',
                message: 'User not authenticated'
              });
            } // Checks the provider the user lastly authenticated with


            return this.user.getIdTokenResult().then(function (result) {
              if (result.signInProvider !== 'password') {
                // Setup the provider 
                var authProvider = _this26.setupProvider(result.signInProvider);

                if (authProvider === null) {
                  return Promise.reject({
                    code: 'auth/unsupported-provider',
                    message: 'Unsupported provider'
                  });
                } // Re-authenticate the user with the same provider


                return _this26.user.reauthenticateWithPopup(authProvider).then(function (credentials) {
                  return credentials.user;
                });
              }

              console.log("Refreshing authentication: ", _this26.user.email); // Gets fresh credentials for the current user

              var credential = firebase_app__WEBPACK_IMPORTED_MODULE_2__["auth"].EmailAuthProvider.credential(_this26.user.email, password); // Re-authenticate the user with the fresh credentials

              return _this26.user.reauthenticateWithCredential(credential).then(function (credential) {
                return credential.user;
              });
            });
          }
          /** Signs out */

        }, {
          key: "signOut",
          value: function signOut() {
            console.log("Signing-out");
            return this.auth.signOut();
          }
          /** Applies the received action code to complete the requested action */

        }, {
