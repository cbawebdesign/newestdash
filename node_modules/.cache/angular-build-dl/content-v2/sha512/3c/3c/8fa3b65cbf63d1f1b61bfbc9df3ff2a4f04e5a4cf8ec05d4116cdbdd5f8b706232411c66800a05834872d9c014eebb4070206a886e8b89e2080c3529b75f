/*! exports provided: DatabaseBatch */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "DatabaseBatch", function() { return DatabaseBatch; });
class DatabaseBatch {
    constructor(db, btc) {
        this.db = db;
        this.btc = btc;
    }
    /**
     * Creates / destructively re-writes the document content.
     * Adds the 'created' timestamp
     */
    set(ref, data) {
        const created = this.db.timestamp;
        return this.btc.set(ref, Object.assign(Object.assign({}, data), { created })), this;
    }
    /**
     * Updates the document content by merging the new data with the existing one including sub objects.
     * Adds / updates the 'updated' timestamp
     */
    merge(ref, data) {
        const updated = this.db.timestamp;
        return this.btc.set(ref, Object.assign(Object.assign({}, data), { updated }), { merge: true }), this;
    }
    /**
     * Updates the document content with the new data. Unlike merge, it overwrites sub objects.
     * Adds / updates the 'updated' timestamp
     */
    update(ref, data) {
        const updated = this.db.timestamp;
        return this.btc.update(ref, Object.assign(Object.assign({}, data), { updated })), this;
    }
    /** Deletes the document */
    delete(ref) {
        return this.btc.delete(ref), this;
    }
    /** Commits the batch for multiple writes */
    commit() {
        return this.btc.commit();
    }
}


