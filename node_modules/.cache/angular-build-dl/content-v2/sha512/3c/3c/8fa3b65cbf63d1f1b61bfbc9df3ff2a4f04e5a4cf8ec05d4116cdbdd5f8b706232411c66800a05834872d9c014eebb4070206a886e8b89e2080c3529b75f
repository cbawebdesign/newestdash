        _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵprojectionDef"]();
        _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵtemplate"](0, ReadmeComponent_ng_content_0_Template, 1, 0, "ng-content", 0);
        _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵtemplate"](1, ReadmeComponent_ng_container_1_Template, 9, 8, "ng-container", 1);
    } if (rf & 2) {
