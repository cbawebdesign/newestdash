      var _angular_core__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(
      /*! @angular/core */
      "8Y7J");
      /** Default ContentSelector reverting to the browser language if 'auto' has been specified  */


      var ContentSelector = /*#__PURE__*/function () {
        function ContentSelector(router, config) {
          _classCallCheck(this, ContentSelector);

          this.router = router;
          this.config = config;
        }

        _createClass(ContentSelector, [{
          key: "canActivate",
          value: function canActivate(route, state) {
            // Gets the language code requested from the route 
            var requested = this.requestedValue(route);
            console.log('Requested language:', requested); // Selects the best language among the allowed ones 

            var selected = this.valueAllowed(requested === 'auto' ? this.browserLanguage : requested);
            console.log('Selected language:', selected); // Keeps track of the currently selected language within the ContentConfigurator for other services to take advantage from

            this.config.currentValue = selected; // Redirects to the selected whenever differs from the requested

            return requested === selected || this.router.createUrlTree([selected]);
          }
          /** Gets the requested language value from the route */

        }, {
          key: "requestedValue",
          value: function requestedValue(route) {
            return route.paramMap.get(this.config.selector);
          }
          /** Checks if the language code is among the allowed ones */

        }, {
          key: "isValueAllowed",
          value: function isValueAllowed(value) {
            return !!this.config.supportedValues.find(function (allowed) {
              return allowed === value;
            });
          }
          /** Filters the language code ensuring is among the allowed ones returning the default value otherwise*/

        }, {
          key: "valueAllowed",
          value: function valueAllowed(value) {
            return this.config.supportedValues.find(function (allowed) {
              return allowed === value;
            }) || this.config.defaultValue;
          }
          /** Two digits browser language code */

        }, {
          key: "browserLanguage",
          get: function get() {
            var detected = this.detectLanguage().split('-')[0];
            console.log("Browser language:", detected);
            return detected;
          }
          /** Detects the preferred language according to the browser, whenever possible */

        }, {
          key: "detectLanguage",
          value: function detectLanguage() {
            var navigator = !!window && window.navigator || {};
            return (navigator.languages ? navigator.languages[0] : null) || navigator.language || navigator.browserLanguage || navigator.userLanguage || '';
          }
        }]);

        return ContentSelector;
      }();

