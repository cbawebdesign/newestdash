        function ContentLoader(config, http) {
          _classCallCheck(this, ContentLoader);

          this.config = config;
          this.http = http;
          /** Content data observable */

          this.data$ = new rxjs__WEBPACK_IMPORTED_MODULE_3__["ReplaySubject"](1);
        }
        /** Returns the cached data */


        _createClass(ContentLoader, [{
          key: "cache",
          get: function get() {
            return this.data || this.flush();
          }
          /** Returns the current language as a two digit code */

        }, {
          key: "language",
          get: function get() {
            return this.cache.lang;
          }
          /** Flushes the cache */

        }, {
          key: "flush",
          value: function flush() {
            var lang = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : this.config.defaultValue;
            return this.data = {
              lang: lang
            };
          }
          /** Checks if the requested language is allowed reverting to the default one otherwise  */

        }, {
          key: "languageAllowed",
          value: function languageAllowed(lang) {
            return this.config.supportedValues.find(function (allowed) {
              return allowed === lang;
            }) || this.config.defaultValue;
          }
          /** Load the requested module in the specified language
           * param lang the two digit language ContentLoader
           * param moduleName the json file name to load
           */

        }, {
          key: "loadFile",
          value: function loadFile(path, lang, name) {
            var _this28 = this;

            // Flushes the cache when switching language
            if (lang !== this.language) {
              this.flush(lang);
            } // Returns the data from the cache whenever possible


            if (!!this.cache[name]) {
              return Object(rxjs__WEBPACK_IMPORTED_MODULE_3__["of"])(this.cache[name]);
            } // Let's load


            console.log('loading:', path, lang, name); // Always load the default module together with the requested one

            return Object(rxjs__WEBPACK_IMPORTED_MODULE_3__["forkJoin"])( // Default language file (full)
            this.http.get("".concat(path, "/").concat(this.config.defaultValue, "/").concat(name, ".json")), // Loads the requested language file only when differs from he default language
            this.language !== this.config.defaultValue ? // Requested language file (partial)
            this.http.get("".concat(path, "/").concat(this.language, "/").concat(name, ".json")).pipe( // Reverts to the default language in case of errors (basically it pass an empty object 
            // since default content will be merged in the next map() )
            Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_1__["catchError"])(function () {
              return Object(rxjs__WEBPACK_IMPORTED_MODULE_3__["of"])({});
            }) // Skips unnecessary loading
            ) : Object(rxjs__WEBPACK_IMPORTED_MODULE_3__["of"])({})).pipe( // Packs the result by merging the modules whenever necessary
            Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_1__["zip"])(function (data) {
              return _this28.language === _this28.config.defaultValue ? data[0] : _this28.merge(data[1], data[0]);
            }), // Caches the content for further requests
            Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_1__["tap"])(function (data) {
              _this28.cache[name] = data;

              _this28.data$.next(_this28.cache);
            }));
          }
          /** Deep merges the localModule with the defaultModule */

        }, {
          key: "merge",
          value: function merge(localModule, defaultModule) {
            var _this29 = this;

            // Done recurring when default module is no longer available
            if (!!defaultModule) {
              // Treat arrays as a special case, so, to keep them as array
              if (Array.isArray(defaultModule)) {
                // Overrides the locale array with tht default when they do not match
                if (!Array.isArray(localModule) || localModule.length !== defaultModule.length) {
                  localModule = defaultModule;
                } else {
                  // Recurs down the tree merging the array content
                  defaultModule.forEach(function (_, index) {
                    return localModule[index] = _this29.merge(localModule[index], defaultModule[index]);
                  });
                }
              } else {
                // Merges the object properties otherwise
                Object.keys(defaultModule).forEach(function (key) {
                  // Copies each missing property
                  if (!localModule[key]) {
                    localModule[key] = defaultModule[key];
                  } // Recurs down the children properties otherwise
                  else if (typeof localModule[key] === 'object') {
                      localModule[key] = _this29.merge(localModule[key], defaultModule[key]);
                    }
                });
              }
            } // Returns the merged object


            return localModule;
          }
        }]);

        return ContentLoader;
      }();

