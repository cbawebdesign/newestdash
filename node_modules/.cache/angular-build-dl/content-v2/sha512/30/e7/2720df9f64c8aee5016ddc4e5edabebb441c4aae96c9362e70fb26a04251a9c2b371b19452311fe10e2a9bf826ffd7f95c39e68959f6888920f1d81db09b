          return new (t || ContentRouterModule)(_angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵinject"](_angular_router__WEBPACK_IMPORTED_MODULE_1__["ROUTES"]), _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵinject"](_loader_content_configurator_service__WEBPACK_IMPORTED_MODULE_2__["ContentConfigurator"]));
