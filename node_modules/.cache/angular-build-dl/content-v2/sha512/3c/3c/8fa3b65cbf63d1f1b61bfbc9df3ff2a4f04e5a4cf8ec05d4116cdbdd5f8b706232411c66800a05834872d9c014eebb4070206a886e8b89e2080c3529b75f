    /** Returns the document object path relative to the database root */
    get path() { return this.ref && this.ref.path; }
    /** Returns the parent collection */
    get parent() {
        return this.ref && this.db.collection(this.ref.parent);
    }
    /** Returns a child collection */
    collection(path) {
        return this.ref && this.db.collection(this.ref.collection(path));
    }
    /** Returns a child distributed counter */
    counter(path, shards) {
        return this.ref && this.db.counter(this.ref.collection(path), shards);
    }
    /**
     * Creates / destructively re-writes the document content.
     * Adds the 'created' timestamp
     */
