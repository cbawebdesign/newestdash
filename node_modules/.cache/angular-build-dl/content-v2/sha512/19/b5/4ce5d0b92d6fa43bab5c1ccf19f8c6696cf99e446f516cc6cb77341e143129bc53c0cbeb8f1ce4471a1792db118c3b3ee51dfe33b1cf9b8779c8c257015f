class DonateComponent {
    constructor(stripe, functions, dark) {
        this.stripe = stripe;
        this.functions = functions;
        this.email = '';
        this.name = '';
        this.currency = 'eur';
        this.ready = false;
        this.progress = false;
        this.completed = false;
        this.defaultAmoutOptions = [
            { label: "5", value: 5 },
            { label: "25", value: 25 },
            { label: "100", value: 100 }
        ];
        // createPaymentIntent runs server side on cloudFunctions
        this.createPaymentIntent = this.functions.callable('createPaymentIntent');
        // Uses an observable to refresh the Card Element automatic style detection on theme changes
        this.autoMode$ = dark.pipe(Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_0__["switchMap"])(() => Object(rxjs__WEBPACK_IMPORTED_MODULE_5__["of"])('auto').pipe(Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_0__["delay"])(0), Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_0__["startWith"])({}))));
    }
    /** Toggles the curency between EUR and USD */
    toggleCurrency() {
        this.currency = this.currency === 'eur' ? 'usd' : 'eur';
    }
    // Process the payment
    pay() {
        this.completed = false;
        this.progress = true;
        this.error = '';
        console.log("Creating payment intent for", this.amount, this.currency);
        // Starts by creating the payment intent server side
        this.createPaymentIntent({
            // Amount goes in cents
            amount: this.amount * 100,
            currency: this.currency,
            // Enables testMode when in developent so the private test key will be used instead of the live one.
            testMode: !env_environment__WEBPACK_IMPORTED_MODULE_6__["environment"].production
        }).then(intent => {
            console.log("Confirming payment intent", intent.id);
            // Once creates, use the client_secret to confirm the intent with the credit card details
            // from the card element
            return this.stripe.confirmCardPayment(intent.client_secret, {
                payment_method: {
                    card: this.card,
                    billing_details: {
                        name: this.name,
                        email: this.email
                    }
                }
            });
        }).then(result => {
            var _a, _b;
            console.log("Transaction completed", (_a = result.paymentIntent) === null || _a === void 0 ? void 0 : _a.status);
            // Tracks the errors, if any
            this.error = (_b = result.error) === null || _b === void 0 ? void 0 : _b.message;
            // Stops the progress
            this.progress = false;
            // Displays the completion 
            this.completed = !this.error;
            // Clears the card
            this.card.clear();
        }).catch((e) => {
            console.log("Transaction terminated", e);
            // Tracks the errors, if any
            this.error = e.message || e.code;
            // Stops the progress
            this.completed = this.progress = false;
            // Clears the card
            this.card.clear();
        });
    }
}
