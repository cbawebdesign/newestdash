/** DarkMode observer. Emits true whenever the dark mode is detected */
class DarkModeObserver extends rxjs__WEBPACK_IMPORTED_MODULE_0__["Observable"] {
    constructor(document) {
        // Creates an observble combining the forcing flag with the media query
        super(subscriber => Object(rxjs__WEBPACK_IMPORTED_MODULE_0__["combineLatest"])(this.force$, this.darkObserver().pipe(Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_1__["startWith"])(null))).pipe(
